<?xml version='1.0' encoding='utf-8'?>
<widget id="com.relaxpanama.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Relax Panama</name>
    <description>
        App para relajarse.
    </description>
    <author email="ingjeanpaulrojas@gmail.com" href="http://ingjprojas.zz.com.ve">
        Jean Paul Rojas
    </author>
    <content src="index.html" />
    <icon src="www/img/favicon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="android-minSdkVersion" value="20" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="KeepRunning" value="true" />
</widget>
